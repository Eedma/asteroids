    "url": "https://github.com/Eedma/asteroids/issues"
  },
  "homepage": "https://github.com/Eedma/asteroids#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "netlify-cli": "^2.19.0",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.10.3"
  },
